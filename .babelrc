{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@styles": "./src/styles",
          "@main-types": "./src/types",
          "@store": "./src/store",
          "@config": "./src/config",
          "@hooks": "./src/hooks",
          "@api": "./src/api",
          "@pages": "./src/pages",
          "@constants": "./src/constants"
        }
      }
    ]
  ]
}
